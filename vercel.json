{
    "rewrites": [
        { "source": "/", "destination": "/index.html" },
        { "source": "/index.html", "destination": "/index.html" },
        { "source": "/pricing.html", "destination": "/pricing.html" },
        { "source": "/aboutus.html", "destination": "/aboutus.html" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/404.html",
            "status": 404
        }
    ]
}
